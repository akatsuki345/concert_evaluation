<h2>Customers search for "<%= params[:word] %>"</h2>
<table class="table table-hover table-inverse">
  <thead>
    <tr>
      <th>name</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <% if @range == "Customer" %>
    <tbody>
      <% @customers.each do |customer| %>
        <tr>
          <%#= image_tag user.get_profile_image, size: '50x50' %>
          <td><%= customer.first_name %></td>
        </tr>
      <% end %>
    </tbody>
  <% end %>
</table>
